        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="RadioGroup.Vertical.Full">
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="RadioGroup.Vertical.Wrap">
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="RecyclerView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>
    <style name="ScrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ScrollView.Vertical">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ScrollView.Vertical.Full">
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="ScrollView.Vertical.Wrap">
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SearchView" parent="Widget.AppCompat.SearchView">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="Spinner" parent="@android:style/Widget.Spinner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawSelectorOnTop">false</item>
    </style>
    <style name="SwipeRefreshLayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="TableLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:stretchColumns">0</item>
        <item name="android:shrinkColumns">*</item>
    </style>
    <style name="TableRow">
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>
    <style name="TextAppearance.ActionBar.Title" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextView" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
    </style>
    <style name="TextView.Description">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:textColor">@color/text_light</item>
    </style>
    <style name="TextView.Description.Center">
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>
    <style name="TextView.Description.Truncate">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextView.Empty">
        <item name="android:visibility">gone</item>
        <item name="android:textSize">@dimen/text_size_large</item>
        <item name="android:textStyle">italic</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>
    <style name="TextView.Error">
        <item name="android:textSize">@dimen/text_size_large</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">@color/text_error</item>
        <item name="android:visibility">gone</item>
    </style>
    <style name="TextView.Info">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">italic</item>
    </style>
    <style name="TextView.Label">
        <item name="android:textSize">@dimen/text_size_large</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">2dp</item>
    </style>
    <style name="TextView.Links">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:linksClickable">true</item>
        <item name="android:autoLink">web|email</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>
    <style name="TextView.Links.Center">
        <item name="android:gravity">center</item>
    </style>
    <style name="TextView.Loading">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">italic</item>
        <item name="android:gravity">center</item>
        <item name="android:visibility">gone</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:textColor">@color/text_light</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="TextView.Prefix">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>
    <style name="TextView.Required">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">@color/text_required</item>
        <item name="android:visibility">gone</item>
    </style>
    <style name="TextView.SubTitle">
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:paddingTop">0dp</item>
    </style>
    <style name="TextView.SubTitle.Truncate">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextView.Suffix">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>
    <style name="TextView.Title">
        <item name="android:textSize">@dimen/text_size_large</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:textColor">@color/text_dark</item>
    </style>
    <style name="TextView.Title.Center">
        <item name="android:gravity">center</item>
    </style>
    <style name="TextView.Title.Truncate">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextView.Value">
        <item name="android:textSize">@dimen/text_size_large</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">16dp</item>
    </style>
    <style name="TimePicker">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:spinnersShown">true</item>
        <item name="android:calendarViewShown">false</item>
    </style>
    <style name="Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">@color/color_primary</item>
    </style>
    <style name="VideoView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_gravity">fill_vertical|fill_horizontal</item>
    </style>
    <style name="ViewPager">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
</resources>